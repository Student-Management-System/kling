{
	"$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
	"spaces": 2,
	"generator-cli": {
		"version": "5.1.1",
		"generators": {
			"student-mgmt-api": {
				"generatorName": "typescript-angular",
				"glob": "openapi-student-mgmt.json",
				"output": "#{cwd}/libs/shared/data-access/student-mgmt-ng-client/src/lib/api",
				"additionalProperties": {
					"ngVersion": "12.0.3",
					"supportsES6": "true"
				}
			}
		}
	}
}
