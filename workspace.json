{
	"version": 1,
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"defaultProject": "client",
	"schematics": {
		"@nrwl/angular:application": {
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress",
			"strict": true
		},
		"@nrwl/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {}
	},
	"projects": {
		"client": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "apps/client",
			"sourceRoot": "apps/client/src",
			"prefix": "kling",
			"architect": {
				"build": {
					"builder": "@angular-builders/custom-webpack:browser",
					"customWebpackConfig": {
						"path": "./apps/client/webpack.config.cjs",
						"mergeStrategies": {
							"externals": "replace"
						}
					},
					"options": {
						"outputPath": "dist/apps/client",
						"index": "apps/client/src/index.html",
						"main": "apps/client/src/main.ts",
						"polyfills": "apps/client/src/polyfills.ts",
						"tsConfig": "apps/client/tsconfig.app.json",
						"aot": true,
						"assets": [
							"apps/client/src/favicon.ico",
							"apps/client/src/assets",
							"apps/client/src/env.js",
							{
								"glob": "**/*",
								"input": "./apps/client/src/app/ide/editor/components/code-editor/grammars",
								"output": "./grammars/"
							},
							{
								"glob": "**/*",
								"input": "./apps/client/src/app/ide/editor/components/code-editor/configurations",
								"output": "./configurations/"
							},
							{
								"glob": "**/*",
								"input": "./apps/client/src/app/ide/editor/components/code-editor/vscode-oniguruma",
								"output": "./node_modules/vscode-oniguruma/"
							},
							{
								"glob": "**/*",
								"input": "./apps/client/src/app/ide/editor/components/code-editor/gen",
								"output": "./workers/"
							}
						],
						"styles": [
							"apps/client/src/styles.scss",
							"apps/client/src/custom-theme.scss",
							"node_modules/ngx-toastr/toastr.css"
						],
						"scripts": ["node_modules/marked/lib/marked.js"]
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/client/src/environments/environment.ts",
									"with": "apps/client/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "client:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "client:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "client:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/client/src/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/client/jest.config.js",
						"passWithNoTests": true
					},
					"outputs": ["coverage/apps/client"]
				}
			}
		},
		"client-e2e": {
			"root": "apps/client-e2e",
			"sourceRoot": "apps/client-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/client-e2e/cypress.json",
						"tsConfig": "apps/client-e2e/tsconfig.e2e.json",
						"devServerTarget": "client:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "client:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"shared-data-access-api-rest-ng-client": {
			"projectType": "library",
			"root": "libs/shared/data-access/api-rest-ng-client",
			"sourceRoot": "libs/shared/data-access/api-rest-ng-client/src",
			"prefix": "kling",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/data-access/api-rest-ng-client/src/**/*.ts"
						]
					}
				}
			},
			"schematics": {}
		},
		"api-rest": {
			"root": "apps/api-rest",
			"sourceRoot": "apps/api-rest/src",
			"projectType": "application",
			"prefix": "api-rest",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"options": {
						"outputPath": "dist/apps/api-rest",
						"main": "apps/api-rest/src/main.ts",
						"tsConfig": "apps/api-rest/tsconfig.app.json",
						"webpackConfig": "apps/api-rest/webpack.config.js"
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": []
						}
					},
					"outputs": ["{options.outputPath}"]
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "api-rest:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/api-rest/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/api-rest/jest.config.js",
						"passWithNoTests": true
					},
					"outputs": ["coverage/apps/api-rest"]
				}
			}
		},
		"code-runner-service": {
			"root": "apps/code-runner-service",
			"sourceRoot": "apps/code-runner-service/src",
			"projectType": "application",
			"prefix": "code-runner-service",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"options": {
						"outputPath": "dist/apps/code-runner-service",
						"main": "apps/code-runner-service/src/main.ts",
						"tsConfig": "apps/code-runner-service/tsconfig.app.json"
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": []
						}
					},
					"outputs": ["{options.outputPath}"]
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "code-runner-service:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/code-runner-service/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/code-runner-service/jest.config.js",
						"passWithNoTests": true
					},
					"outputs": ["coverage/apps/code-runner-service"]
				}
			}
		},
		"client-data-access-state": {
			"projectType": "library",
			"root": "libs/client/data-access/state",
			"sourceRoot": "libs/client/data-access/state/src",
			"prefix": "kling",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/client/data-access/state/src/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/client/data-access/state/jest.config.js",
						"passWithNoTests": true
					},
					"outputs": ["coverage/libs/client/data-access/state"]
				}
			},
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			}
		},
		"shared-data-access-student-mgmt-ng-client": {
			"projectType": "library",
			"root": "libs/shared/data-access/student-mgmt-ng-client",
			"sourceRoot": "libs/shared/data-access/student-mgmt-ng-client/src",
			"prefix": "kling",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:ng-packagr-lite",
					"outputs": ["dist/libs/shared/data-access/student-mgmt-ng-client"],
					"options": {
						"project": "libs/shared/data-access/student-mgmt-ng-client/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "libs/shared/data-access/student-mgmt-ng-client/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "libs/shared/data-access/student-mgmt-ng-client/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/shared/data-access/student-mgmt-ng-client/src/**/*.ts",
							"libs/shared/data-access/student-mgmt-ng-client/src/**/*.html"
						]
					}
				}
			}
		}
	}
}
